            GG.All_UI = function(a)
                for _, v in pir(GetDescendants(game)) do
                    if IsA(v, "BillboardGui") then
                        v:Destroy(true); 
                    end;
                    if IsA(v, "ScreenGui") then
                        v.Enabled = a; 
                    end;
                end;
                return;
            end;
            GG.self_UI = function(a)
                for _, v in pir(GetDescendants(selff)) do
                    if IsA(v, "BillboardGui") then
                        v:Destroy(true); 
                    end;
                    if IsA(v, "ScreenGui") then
                        v.Enabled = a; 
                    end;
                end;
                return;
            end;

