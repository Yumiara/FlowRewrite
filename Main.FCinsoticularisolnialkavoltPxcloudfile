                OpeningLine.Size = UDim2.new(0, 0, 0, 3)
                OpeningLine.Position = UDim2.new(0, 0, 0.5, 0)
                OpeningLine.AnchorPoint = Vector2.new(0, 0.5)
                OpeningLine.BackgroundColor3 = Color3.fromRGB(200, 150, 255)
